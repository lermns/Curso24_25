<UserControl x:Class="impuesto.UserControl1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <StackPanel>
            <TextBox x:Name="textBoxNumero" Margin="5" HorizontalAlignment="Left" Width="150" LostFocus="textBoxNumero_LostFocus"/>
            <TextBox x:Name="textBoxResultado" Margin="5" HorizontalAlignment="Left" Width="150" IsReadOnly="True"/>
        </StackPanel>
    </Grid>
</UserControl>
