<Window x:Class="impuesto.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:impuesto"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <StackPanel>
            <Label Content="Tabaco del 21%"/>
            <local:UserControl1 x:Name="tabacoControl" HorizontalAlignment="Left" VerticalAlignment="Top" TipoIVA="0.21"/>

            <Label Content="Bombilla del 10%"/>
            <local:UserControl1 x:Name="bombillaControl" HorizontalAlignment="Left" VerticalAlignment="Top" TipoIVA="0.10"/>

            <Label Content="Coche del 4%"/>
            <local:UserControl1 x:Name="cocheControl" HorizontalAlignment="Left" VerticalAlignment="Top" TipoIVA="0.04"/>

            <Label Content="Vino del 10%"/>
            <local:UserControl1 x:Name="vinoControl" HorizontalAlignment="Left" VerticalAlignment="Top" TipoIVA="0.10"/>
        </StackPanel>
    </Grid>
</Window>